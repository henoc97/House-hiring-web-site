        <div class="btns">
            <div class="btn-home">Retourner à l'accueil</div>
            <div class="btn-sign">Retourner au login</div>
        </div>
    </div>
    <script src="/public/endpoints.js"></script>
    <script src="/admin/admin-error.js"></script>
</body>
</html>