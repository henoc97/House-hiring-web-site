<div class="card">
    <div class="card-header">
      <h2>Enregistrer un locataire</h2>
    </div>
    <form id="tenant-form">
        <div class="form-group">
            <label for="property-option">Propriétés à louer:</label>
            <select id="property-option" name="property" required>
              
            </select>
          </div>
        <div class="form-group">
            <label for="tenant-lastname">Nom:</label>
            <input type="text" id="tenant-lastname" name="lastname" required>
        </div>
        <div class="form-group">
            <label for="tenant-firstname">Prénoms:</label>
            <input type="text" id="tenant-firstname" name="firstname" required>
        </div>
        <div class="form-group">
            <label for="tenant-contact-moov">Contact moov:</label>
            <input type="text" id="tenant-contact-moov" name="contact-moov" >
        </div>
        <div class="form-group">
            <label for="tenant-contact-tg">Contact togocom:</label>
            <input type="text" id="tenant-contact-tg" name="contact-tg" >
        </div>
        
        <button type="submit">Enregistrer</button>
    </form>
  </div>
  