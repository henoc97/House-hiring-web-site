<div class="card">
  <div class="card-header">
    <h2>Commander un reçu </h2>
  </div>
  <form id="receipt-form">
    <div class="form-group">
      <label for="tenants-properties-option">Locataire & Logement:</label>
      <select id="tenants-properties-option" name="tenants-properties" required>
      
      </select>
    </div>
    <div class="form-group">
      <label for="receipt-months">Mois:</label>
      <select id="receipt-months" name="months" multiple="multiple" required>
        
      </select>
    </div>
    <div class="form-group">
      <label for="receipt-sumpayed">Coût:</label>
      <input type="number" id="receipt-sumpayed" name="sumpayed" required>
    </div>
    
    <button type="submit">Enregistrer</button>
  </form>
</div>
  
  